<template name="starsTemplate">
  <view class='stars-container'>
    <view class='stars'>
      <block wx:for="{{star}}" wx:for-item="i" wx:key='n'>
        <image src="{{i==1?'/images/icon/star.png':'/images/icon/none-star.png'}}"></image>
      </block>
    </view>
    <text class='star-score'>{{score}}</text>
  </view>
</template>